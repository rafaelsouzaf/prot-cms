<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.github.rafaelsouzaf.prot.client.view.product.category.CategoryModel" table="pd_product_category" dynamic-update="true" dynamic-insert="true" select-before-update="false">
		<id name="categoryId">
			<generator class="native" />
		</id>
		<property name="categoryName" />
		<property name="categoryDescription" />
		<property name="categoryCreateDate" type="timestamp"/>
		<property name="categoryModifiedDate" type="timestamp"/>
		<property name="idx" type="int"/>
	</class>
</hibernate-mapping>
